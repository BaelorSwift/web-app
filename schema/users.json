{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"required": [
		"name",
		"email_address",
		"password"
	],
	"additionalProperties": false,
	
	"properties": {
		"name": {
			"type": "string",
			"minLength": 1
		},
		"email_address": {
			"type": "string",
			"minLength": 1
		},
		"password": {
			"type": "string",
			"minLength": 12
		}
	}
}
